[windows]

[windows:children]
win_us
win_canada

[win_us]
server1.us.example.com

[win_canada]
server1.canada.example.com

[windows:vars]
ansible_connection=winrm
ansible_winrm_transport=kerberos
ansible_winrm_server_cert_validation=ignore
ansible_port=5985

[win_us:vars
ansible_user="svc.ansible@US.EXAMPLE.COM"
ansible_password="SuperSecretPassword"

[win_canada:vars]
ansible_user="svc.ansible@CANADA.EXAMPLE.COM"
ansible_password="AnotherSuperSecretPassword"
